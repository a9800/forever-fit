import sqlite3

conn = sqlite3.connect('forever-fit.db')

conn.execute('''CREATE TABLE USERS
         (UNAME         VARCHAR(15)    NOT NULL,
         FNAME          TEXT           NOT NULL,
         LNAME          TEXT           NOT NULL,
         DOB            VARCHAR(20)    NOT NULL,
         PASSWORD       VARCHAR(50)    NOT NULL,
         ISTRAINER      BOOL,
         FITNESS_GOALS  TEXT,
         PRIMARY KEY(UNAME));''')




